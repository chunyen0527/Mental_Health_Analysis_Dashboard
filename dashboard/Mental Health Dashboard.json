          "rawQuery": true,
          "rawSql": "SELECT \r\n  CASE\r\n    WHEN stress_frequency = 'Never' THEN 'Never'\r\n    WHEN stress_frequency = 'Rarely' THEN 'Rarely'\r\n    WHEN stress_frequency = 'Monthly' THEN 'Monthly'\r\n    WHEN stress_frequency = 'weekly' THEN 'Weekly'\r\n    WHEN stress_frequency = 'Daily' THEN 'Daily'\r\n    ELSE stress_frequency\r\n  END AS stress_frequency_label,\r\n  COUNT(CASE WHEN mental_health_rating = 'Very Poor' THEN productivity_rating ELSE NULL END) AS `Very Poor`,\r\n  COUNT(CASE WHEN mental_health_rating = 'Average' THEN productivity_rating ELSE NULL END) AS Average,\r\n  COUNT(CASE WHEN mental_health_rating = 'Excellent' THEN productivity_rating ELSE NULL END) AS Excellent,\r\n  COUNT(CASE WHEN mental_health_rating = 'Great' OR mental_health_rating = 'Great and Happy about it' THEN productivity_rating ELSE NULL END) AS Great\r\nFROM `mental-health-analysis-468004.dataset1.mental_health_data`\r\nGROUP BY stress_frequency\r\nORDER BY CASE\r\n    WHEN stress_frequency = 'Never' THEN 1\r\n    WHEN stress_frequency = 'Rarely' THEN 2\r\n    WHEN stress_frequency = 'Monthly' THEN 3\r\n    WHEN stress_frequency = 'Weekly' THEN 4\r\n    WHEN stress_frequency = 'Daily' THEN 5\r\n    ELSE 6\r\nEND",
